    "changeRoutine": [
      {
        "Note": "Determine if user has pressed Enter key.",
        "func": "GET",
        "collection": "thisButton",
        "property": "text",
        "variable": "myChat"
      },
      {
        "func": "COMPUTE",
        "operation": "includes",
