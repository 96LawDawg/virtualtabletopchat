          }
        ],
        "operand2": "\n",
        "variable": "isCR"
      },
      {
        "Note": "If user has pressed Enter, do the thenRoutine; otherwise do nothing.",
        "func": "IF",
        "applyVariables": [
          {
            "parameter": "operand1",
            "variable": "isCR"
          }
        ],
        "operand2": true,
        "relation": "==",
        "thenRoutine": [
          {
            "Note": "Erase the carriage return.",
            "func": "COMPUTE",
            "operation": "replace",
            "applyVariables": [
              {
                "parameter": "operand1",
                "variable": "myChat"
              }
            ],
            "operand2": "\n",
            "operand3": "",
            "variable": "myChat"
          },
          {
            "func": "SET",
            "collection": "thisButton",
            "property": "text",
            "applyVariables": [
              {
                "parameter": "value",
                "variable": "myChat"
              }
            ]
          },
          {
            "Note": "Get the current chat log string.",
            "func": "SELECT",
            "property": "vttChatID",
            "value": "mainChat"
          },
          {
            "func": "SELECT",
            "source": "DEFAULT",
            "property": "owner",
            "value": null
          },
          {
            "func": "GET",
            "property": "text",
            "variable": "mainChat"
          },
          {
            "Note": "Add the player's typed entry to the beginning of the string.",
            "func": "COMPUTE",
            "operation": "concat",
            "variable": "mainChat",
            "applyVariables": [
              {
                "parameter": "operand1",
                "variable": "myChat"
              },
              {
                "parameter": "operand2",
                "variable": "mainChat"
              }
            ]
          },
          {
            "Note": "Get the user unicode color icon and attach it to the playerName",
            "func": "GET",
            "collection": "thisButton",
            "property": "chatColor",
            "variable": "chatColor"
          },
          {
            "func": "COMPUTE",
            "operation": "+",
            "applyVariables": [
              {
                "parameter": "operand1",
                "variable": "chatColor"
              },
              {
                "parameter": "operand2",
                "template": "{playerName}: "
              }
            ],
            "variable": "displayName"
          },
          {
            "Note": "Create a string with the display name and the new chat log string.",
            "func": "COMPUTE",
            "operation": "concat",
            "applyVariables": [
              {
                "parameter": "operand1",
                "variable": "displayName"
              },
              {
                "parameter": "operand2",
                "variable": "mainChat"
              }
            ],
            "variable": "mainChat"
          },
          {
            "Note": "Add a blank line at the top of the chat log.",
            "func": "COMPUTE",
            "operation": "concat",
            "variable": "mainChat",
            "applyVariables": [
              {
                "parameter": "operand2",
                "variable": "mainChat"
              }
            ],
            "operand1": "\n\n"
          },
          {
            "Note": "Select the main chat log and put the chat string into it.",
            "func": "SELECT",
            "property": "vttChatID",
            "value": "mainChat"
          },
          {
            "func": "SELECT",
            "source": "DEFAULT",
            "property": "owner",
            "value": null
          },
          {
            "func": "SET",
            "property": "text",
            "applyVariables": [
              {
                "parameter": "value",
                "variable": "mainChat"
              }
            ]
          },
          {
            "Note": "Select the cloned main chat logs and put the chat string into it.",
            "func": "SELECT",
            "property": "vttChatID",
            "value": "mainChat"
          },
          {
            "func": "SELECT",
            "source": "DEFAULT",
            "property": "owner",
            "relation": "!=",
            "value": null
          },
          {
            "func": "SET",
            "property": "text",
            "applyVariables": [
              {
                "parameter": "value",
                "variable": "mainChat"
              }
            ]
          },
          {
            "Note": "Erase the text in the chat box.",
            "func": "SET",
            "property": "text",
            "collection": "thisButton",
            "value": ""
          }
        ]
      }
