      {
        "func": "GET",
        "collection": "thisButton",
        "property": "leftactivecss",
        "variable": "leftActiveCss"
      },
      {
        "func": "GET",
        "collection": "thisButton",
        "property": "rightactivecss",
        "variable": "rightActiveCss"
      },
      {
        "func": "GET",
        "collection": "thisButton",
